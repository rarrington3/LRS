{
    "presets": [
        "es2015",
        "es2016",
        "es2017"
    ],
    "plugins": [
        "angular2-annotations",
        "transform-export-extensions",
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-flow-comments",
    ],
    "ignore": [
        "(node_modules)"
    ],
    "env": {
        "test": {
            "plugins": [
                [
                    "istanbul",
                    {
                        "exclude": ["www/modules/dev/**/*.js", "www/libs/**/*.js", "**/*.generated.js", "**/*spec.js", "**/*spec.jsx"]
                    }
                ]
            ]
        }
    }
}