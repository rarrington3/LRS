{
    "id": "http://LRS/OptionsItem",
    "type": "object",
    "$schema": "http://json-schema.org/draft-03/schema",
    "required": [
        "code"
    ],
    "additionalProperties": false,
    "properties": {
        "code": {
            "description": "The unique code for this item.",
            "type": "string"
        },
        "description": {
            "description": "The description for this item.",
            "type": "string"
        },
        "options": {
            "description": "The multiple options",
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    }
}
