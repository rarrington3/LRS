  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ],
          "ignore": "pdfjs"
        }
      ],
      [
        "./build/tasks/libs/stringify",
        {
          "match": "(txt|html)$"
        }
      ],
      [
        "./build/tasks/libs/styleify",
        {
          "mode": "return",
          "match": "(css|less|scss|sass)$",
          "autoprefixer": {
            "browsers": [
              "> 1%",
              "last 2 versions",
              "Firefox ESR",
              "Opera 12.1"
            ]
          }
        }
      ],
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "global:$",
    "modernizr": "global:Modernizr"
  }
